{
  "build": {
    "content": [
      {
        "src": "wiki_content",
        "files": [ "**/*.{md,yml}" ],
        "dest": "."
      }
    ],
    "resource": [
      {
        "src": "wiki_content",
        "files": [ "**/*.{jpg,jpeg,png,bmp,gif,svg}" ],
        "dest": "."
      }
    ],
    "externalReference": [],
    "dest": "",
    "title": "Chrysalis Help document",
    "template": [ "default", "docfx/templates/egip" ],
    "serve": false,
    "globalMetadata": {
      "_appTitle": "Chrysalis Help document",
      "_disableContribution": false,
      "_disableBreadcrumb": true,
      "_gitContribute": {
        "repo": "https://servicesdocs.visualstudio.com/Chrysalis/_git/ChrysalisHelp",
        "branch": "WikidocfxEvergreen"
      }
    }
  }
}
