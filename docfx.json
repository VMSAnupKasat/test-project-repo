{
  "build": {
    "content": [
      {
        "src": "docs/",
        "files": ["**/*.{md,yml}"],
        "dest": "."
      }
    ],
    "resource": [
      {
        "src": "docs/attachments/",
        "files": ["**/*.{jpg,jpeg,png,bmp,gif,svg}"],
        "dest": "."
      }
    ],
    "externalReference": [],
    "dest": "_site",
    "title": "Chrysalis Help document",
    "serve": false,
    "globalMetadata": {
      "_appTitle": "Chrysalis Help document",
      "_disableContribution": false,
      "_disableBreadcrumb": true,
      "_gitContribute": {
        "repo": "https://dev.azure.com/servicescode-test/TCS-Test-Project/_git/DocsAsCode",
        "branch": "chrysalis-wiki-help"
      }
    }
  }
}
