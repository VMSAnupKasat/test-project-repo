{
  "build": {
    "contentSecurityPolicy": "default-src 'self'; script-src 'self' 'unsafe-inline';",
    "content": [
      {
        "src": "docs2/",
        "files": [ "**/*.{md,yml}" ],
        "dest": "."
      }
    ],
    "resource": [
      {
        "src": "docs2/",
        "files": [ "**/*.{jpg,jpeg,png,bmp,gif,svg}" ],
        "dest": "."
      }
    ],
    "markdown": {
      "templates": [
        {
          "type": "default",
          "transform": "escape"
        }
      ]
    },
    "externalReference": [],
    "dest": "_site",
    "title": "Evergreen Delivery IP",
    "template": [ "default", "../docfx/templates/egip" ],
    "serve": false,
    "globalMetadata": {
      "_appTitle": "Evergreen Delivery IP",
      "_disableContribution": false,
      "_disableBreadcrumb": true,
      "_gitUrlPattern":"github",
      "_gitContribute": {
        "repo": "https://github.com/VMSAnupKasat/test-project-repo",
        "branch": "evergreen-doc-path"
      }
    }
  }
}
