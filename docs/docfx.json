{
  "build": {
    "content": [
      {
        "src": "docs2/",
        "files": [ "**/*.{md,yml}" ],
        "dest": "."
      }
    ],
    "resource": [
      {
        "src": "docs2/",
        "files": [ "**/*.{jpg,jpeg,png,bmp,gif,svg}" ],
        "dest": "."
      }
    ],
    "externalReference": [],
    "dest": "_site",
    "title": "Evergreen Delivery IP",
    "template": [ "default", "../docfx/templates/egip" ],
    "serve": false,
    "globalMetadata": {
      "_appTitle": "Evergreen Delivery IP",
      "_disableContribution": false,
      "_disableBreadcrumb": true,
      "_gitContribute": {
        "repo": "https://servicesdocs.visualstudio.com/Chrysalis/_git/ChrysalisHelp",
        "branch": "docfx_evergreen_docs"
      }
    }
  }
}
