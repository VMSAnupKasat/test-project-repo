{
  "build": {
    "content": [
      {
        "files": [
          "Index.md",
          "toc.yml",
          "unity.md",
          "ingredient1.md",
          "ingredient2.md",
          "ingredient3.md",
          "ingredient4.md"
        ]
      }
    ],
    "resource": [
      {
        "src": "../",
        "files": [
          "images/**",
          "logo.svg",
          "favicon.ico"          
        ]
      }
    ],
    "dest": "_site",
    "title": "CI Innovation Cookbook Ingredients",
    "template": ["default", "../templates/material"],
    "postProcessors": ["ExtractSearchIndex"],
    "cleanupCacheHistory": true,
    "disableGitFeatures": false,
    "serve": false,
    "globalMetadata": {
      "_appTitle": "CI Innovation Cookbook",
      "_enableSearch": true,
      "_disableContribution": true,
      "_disableBreadcrumb": true,
      "_gitContribute": {
        "repo": "https://chrysalis-innersource.visualstudio.com/defaultcollection/_git/Innovation%20Cookbook",
        "branch": "main"
      }
    }
  }
}